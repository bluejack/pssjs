{
  "name": "pssjs",
  "version": "0.0.1",
  "description": "A javascript client to the pixel starships API",
  "main": "dist/index.js",
  "author": "bluejack",
  "type": "module",
  "license": "Overclocked Free Source",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^8.57.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "dependencies": {
    "strftime": "^0.10.3", 
    "uuid": "^11.0.3",
    "xml2json": "^0.12.0"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "useTabs": false
        }
      },
      {
        "files": ".eslintrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }

}